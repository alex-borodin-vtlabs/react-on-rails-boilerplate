{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.9.0",
    "npm": "4.1.1"
  },
  "scripts": {
    "build:test": "concurrently --kill-others \"webpack -w --config webpack.client.config.js\" \"webpack -w --config webpack.server.config.js\"",
    "build:production": "NODE_ENV=production concurrently --kill-others \"webpack -w --config webpack.client.config.js\" \"webpack -w --config webpack.server.config.js\"",
    "build:development": "concurrently --kill-others \"webpack -w --config webpack.client.config.js\" \"webpack -w --config webpack.server.config.js\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "concurrently": "^3.4.0",
    "es5-shim": "^4.5.9",
    "expose-loader": "^0.7.3",
    "immutable": "^3.8.1",
    "imports-loader": "^0.7.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-on-rails": "6.9.3",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.3.3"
  },
  "devDependencies": {}
}
